{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "regexManagers": [
    {
      "fileMatch": ["^docker/traefik-stack/traefik/config/traefik\\.yml$"],
      "matchStrings": [
        "\\s+moduleName: \"?(?<depName>github\\.com\\/[^\\/\"\\s]+\\/[^\\/\"\\s]+)\"?[\\r\\n\\s]+version: \"?(?<currentValue>v[0-9]+\\.[0-9]+\\.[0-9]+)\"?"
      ],
      "datasourceTemplate": "github-releases",
      "extractVersionTemplate": "^v(?<version>.*)$",
      "depNameTemplate": "{{depName.replace('github.com/', '')}}"
    }
  ]
}
